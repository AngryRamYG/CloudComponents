<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <AssemblyName>AngryMonkey.Cloud.Components</AssemblyName>
    <RootNamespace>AngryMonkey.Cloud.Components</RootNamespace>
		<TypeScriptCompileBlocked>true</TypeScriptCompileBlocked>
		<Product>Cloud Components</Product>
		<Authors>Angry Monkey</Authors>
		<Company>Angry Monkey</Company>
		<Description />
		<PackageLicenseExpression></PackageLicenseExpression>
		<PackageProjectUrl>https://www.angrymonkeycloud.com/</PackageProjectUrl>
		<Version>1.0.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="ProgressBar\progressbar.min.js" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="General\js\DotNet.d.ts" />
    <Content Include="ProgressBar\progressbar.less" />
    <Content Include="ProgressBar\progressbar.ts" />
    <Content Include="VideoPlayer\videoplayer.less" />
    <Content Include="VideoPlayer\videoplayer.ts" />
  </ItemGroup>


  <ItemGroup>
    <SupportedPlatform Include="browser" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="5.0.8" />
  </ItemGroup>

  <ItemGroup>
    <Content Update=".mateconfig.json">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Content>
    <Content Update="tsconfig.json">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <Folder Include="wwwroot\" />
  </ItemGroup>

</Project>
