{
	"builds": [
		{
			"name": "dev",
			"js": {
				"minify": true,
				"sourceMap": false,
				"declaration": false
			}
		},
		{
			"name": "cloudweb",
			"js": {
				"minify": true,
				"sourceMap": false,
				"declaration": false,
				"webClean": true
			}
		}
	],
	"files": [
		{
			"output": "CloudWeb/css/cloud.css",
			"input": "General/css/cloud.less"
		},
		{
			"output": "CloudWeb/js/cloud.js",
			"input": [ "General/js/main.ts", "General/js/common.ts" ],
			"builds": "cloudweb"
		},
		{
			"output": "VideoPlayer/VideoPlayer.razor.css",
			"input": "VideoPlayer/videoplayer.less"
		},
		{
			"output": "wwwroot/videoplayer/videoplayer.js",
			"input": "VideoPlayer/videoplayer.ts"
		},
		{
			"output": "ProgressBar/ProgressBar.razor.css",
			"input": "ProgressBar/progressbar.less"
		},
		{
			"output": "Popup/Popup.razor.css",
			"input": "Popup/popup.less"
		},
		{
			"output": "Dialog/Dialog.razor.css",
			"input": "Dialog/dialog.less"
		},
		{
			"output": "wwwroot/dialog/dialog.js",
			"input": "Dialog/dialog.ts"
		},
		{
			"output": "VolumeBar/VolumeBar.razor.css",
			"input": "VolumeBar/VolumeBar.less"
		},
		{
			"output": "DateTimePicker/DateTimePicker.razor.css",
			"input": "DateTimePicker/DateTimePicker.less"
		},
		{
			"output": "DateTimePicker/Partials/DateTimePickerNavigation.razor.css",
			"input": "DateTimePicker/Partials/DateTimePickerNavigation.less"
		},
		{
			"output": "GridView/GridView.razor.css",
			"input": "GridView/GridView.less"
		},
		{
			"output": "wwwroot/gridview/gridview.js",
			"input": "GridView/GridView.ts"
		}
	]
}